%span.dropdown
  %button.btn.btn-primary.btn-xs.dropdown-toggle{'data-toggle'=> 'dropdown', 'data-target' => '#', href: '#'}
    Manage
    %span.caret
    %span.sr-only Toggle Dropdown
  %nav.dropdown-menu
    %li
      = link_to edit_profile_path(profile) do
        %i.fa.fa-edit
        Edit Member Profile
    %li
      = link_to content_tag('i', '', class: 'fa fa-trash-o') + " Delete",
        profile, confirm: "Are you sure you want to delete the profile?", method: :delete
